{
    "name": "@artifexweb/core",
    "version": "1.0.0",
    "description": "Core styles and scripts",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "test": "jest --verbose ./scripts/**/*.test.js"
    },
    "jest": {
        "transform": {
            "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
        },
        "setupFiles": [
            "./scripts/__tests__/setup.js"
        ]
    },
    "moduleFileExtensions": [
        "js"
    ],
    "files": [
        "scripts/[!__]*.js",
        "styles/**/*"
    ],
    "keywords": [],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "@babel/plugin-transform-modules-commonjs": "^7.15.4",
        "jest": "^27.3.1"
    },
    "dependencies": {
        "@babel/runtime": "^7.15.4"
    }
}
